DROP INDEX if exists berlin2016.idx_berlin2016_routes_id;
DROP INDEX if exists berlin2016.idx_berlin2016_routes_provider;
DROP INDEX if exists berlin2016.idx_berlin2016_routes_timestampstart;
DROP INDEX if exists berlin2016.idx_berlin2016_routes_timestampend;
CREATE INDEX idx_berlin2016_routes_id ON berlin2016.routes(id);
CREATE INDEX idx_berlin2016_routes_provider ON berlin2016.routes(provider);
CREATE INDEX idx_berlin2016_routes_timestampstart ON berlin2016.routes(timestampstart);
CREATE INDEX idx_berlin2016_routes_timestampend ON berlin2016.routes(timestampend);
